<div class="container">
  <h1>Create a new booking</h1>

  <div class="container">
    <%= simple_form_for([@duck, @booking]) do |f| %>
      <%= f.input :status %>
      <%= f.input :start_date %>
      <%= f.input :end_date %>
      <%= f.submit %>
    <% end %>
  </div>

  <%= link_to "Back to the duck list", ducks_path %>
</div>
